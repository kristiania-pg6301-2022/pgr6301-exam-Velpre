{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest --coverage",
    "dev": "parcel watch index.html",
    "clean": "rimraf dist .parcel-cache",
    "build": "npm install --include=dev && npm run build:parcel",
    "build:parcel": "parcel build index.html",
    "format": "prettier --write *.{jsx,js} __tests__/**/*.{jsx,js}",
    "format:check": "prettier --check *.{jsx,js} __tests__/**/*.{jsx,js}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "jest": "^27.5.1",
    "jest-babel": "^1.0.1",
    "parcel": "^2.5.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.jsx",
      "**/*.js",
      "!dist/**",
      "!coverage/**"
    ]
  }
}
